		"@ctx-core/object": "^22.2.1",
		"@ctx-core/svelte": "^12.0.1",
		"resolve": "^1.22.1",
		"svelte": "^3.49.0"
	},
	"devDependencies": {
		"c8": "^7.11.3",
		"check-dts": "^0.6.7",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.6"
