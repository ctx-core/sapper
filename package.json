{
	"name": "@ctx-core/sapper",
	"version": "21.2.24",
	"description": "ctx-core sapper utils",
	"keywords": [
		"ctx-core",
		"sapper"
	],
	"homepage": "https://github.com/ctx-core/sapper#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./asset__http_get",
		"./page",
		"./sapper_preloading",
		"./sapper_session",
		"./_types"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.20",
		"@ctx-core/env": "^17.4.23",
		"@ctx-core/event-log": "^9.2.23",
		"@ctx-core/function": "^21.22.15",
		"@ctx-core/object": "^31.1.1",
		"@ctx-core/svelte": "^15.0.4",
		"svelte": "^4.2.7"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}