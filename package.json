{
	"name": "@ctx-core/sapper",
	"version": "16.0.20",
	"description": "ctx-core sapper utils",
	"keywords": [
		"ctx-core",
		"sapper"
	],
	"homepage": "https://github.com/ctx-core/sapper#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.32",
		"@ctx-core/event-log": "workspace:^5.0.36",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/object": "workspace:^21.0.30",
		"@ctx-core/store": "workspace:^27.0.45",
		"resolve": "^1.20.0",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.9",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"sapper": "0.29.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
