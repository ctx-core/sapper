{
	"name": "@ctx-core/sapper",
	"version": "13.1.25",
	"description": "ctx-core sapper utils",
	"keywords": [
		"ctx-core",
		"sapper"
	],
	"homepage": "https://github.com/ctx-core/sapper#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.1.5",
		"@ctx-core/event-log": "workspace:^4.0.19",
		"@ctx-core/function": "workspace:^19.1.13",
		"@ctx-core/object": "workspace:^20.4.0",
		"@ctx-core/store": "workspace:^26.0.23",
		"resolve": "^1.20.0",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.16",
		"sapper": "0.29.1",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}