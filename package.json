{
	"name": "@ctx-core/sapper",
	"version": "18.0.5",
	"description": "ctx-core sapper utils",
	"keywords": [
		"ctx-core",
		"sapper"
	],
	"homepage": "https://github.com/ctx-core/sapper#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.1.0",
		"@ctx-core/event-log": "workspace:^7.0.5",
		"@ctx-core/function": "workspace:^20.2.20",
		"@ctx-core/nanostores": "workspace:^0.3.0",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/svelte": "workspace:^10.1.1",
		"resolve": "^1.20.0",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123",
		"rimraf": "^3.0.2",
		"sapper": "0.29.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}