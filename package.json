{
	"name": "@ctx-core/sapper",
	"version": "17.0.4",
	"description": "ctx-core sapper utils",
	"keywords": [
		"ctx-core",
		"sapper"
	],
	"homepage": "https://github.com/ctx-core/sapper#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sapper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.36",
		"@ctx-core/event-log": "workspace:^6.0.3",
		"@ctx-core/function": "workspace:^20.2.15",
		"@ctx-core/object": "workspace:^22.0.3",
		"@ctx-core/store": "workspace:^27.0.50",
		"resolve": "^1.20.0",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"sapper": "0.29.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}